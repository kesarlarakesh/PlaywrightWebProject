    "test:hotel:staging": "npx playwright test tests/hotel_e2e_pom.spec.ts --env=staging",
    "test:hotel:headed": "npx playwright test tests/hotel_e2e_pom.spec.ts --headed",
    "test:debug": "npx playwright test --debug"
